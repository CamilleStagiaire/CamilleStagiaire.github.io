{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":  [
        "eslint:recommended",
        "prettier"
        ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
          }
        
    },
    "rules": {
        "no-unused-vars": "off"
    }
}
